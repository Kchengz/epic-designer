{
  "name": "epic-designer-project",
  "version": "1.0.4",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lib": "vitest run && pnpm run build -F epic-designer",
    "test": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "examples:build": "pnpm run build --filter epic-examples",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint . --fix",
    "postinstall": "pnpm -r run stub --if-present"
  },
  "dependencies": {
    "@vueuse/core": "catalog:"
  },
  "devDependencies": {
    "@epic-designer/base-ui": "workspace:*",
    "@epic-designer/core": "workspace:*",
    "@epic-designer/eslint-config": "workspace:*",
    "@epic-designer/hooks": "workspace:*",
    "@epic-designer/manager": "workspace:*",
    "@epic-designer/panel-ui": "workspace:*",
    "@epic-designer/scripts": "workspace:*",
    "@epic-designer/types": "workspace:*",
    "@epic-designer/ui": "workspace:*",
    "@epic-designer/utils": "workspace:*",
    "@turbo/gen": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "epic-designer": "workspace:*",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "less": "catalog:",
    "naive-ui": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitepress": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "packageManager": "pnpm@9.10.0"
}
