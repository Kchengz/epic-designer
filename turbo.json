{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "stub": {
      "cache": false
    },
    "epic-examples#dev": {
      "dependsOn": [
        "@epic-designer/antd#stub",
        "@epic-designer/element-plus#stub",
        "@epic-designer/naive-ui#stub"
      ],
      "cache": false,
      "persistent": true
    },
    "epic-examples#build": {
      "dependsOn": [
        "@epic-designer/antd#build",
        "@epic-designer/element-plus#build",
        "@epic-designer/naive-ui#build"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
